package Star.bro;

public class MethodOverloadSame {
    public static void main(String[] args) {
        byte a = 10;
        byte b = 20;
    }

    public static boolean isSame(byte a, byte b) {
        System.out.println("兩個byte參數方法執行!");
        boolean same;
        if (a == b) {
            same = true;
        } else {
            same = false;
        }
        return same;
    }

    public static boolean isSame(short a, short b) {
        System.out.println("兩個short參數方法執行!");
        boolean same = a == b ? true : false;
        return same;
    }

    public static boolean isSame(int a, int b) {
        System.out.println("兩個int參數方法執行!");
        return a == b;
    }

    public static boolean isSame(long a, long b) {
        System.out.println("兩個long參數方法執行!");
        if (a == b) {
            return true;
        } else {
            return false;
        }
    }
}

